{
  "name": "lifetrack",
  "version": "0.1.1",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --android",
    "dev": "expo start --android",
    "lint": "tsc --noEmit",
    "reset": "expo start --clear",

    "build": "eas build --platform android --profile production",
    "build:preview": "eas build --platform android --profile preview",
    "build:dev": "eas build --platform android --profile development",

    "submit": "eas submit --platform android --latest",

    "release": "eas build --platform android --profile production && eas submit --platform android --latest",

    "builds": "eas build:list"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "expo": "^54.0.33",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "~15.0.8",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-reorderable-list": "^0.18.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "sharp": "^0.34.5",
    "typescript": "~5.9.2"
  },
  "private": true
}
